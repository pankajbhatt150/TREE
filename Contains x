bool isPresent(TreeNode<int>* root, int x) {
        if(root->data==x)
    	return true;
    
    
   bool ans=false;
    for(int i=0;i<root->children.size();i++)
    {
        bool smallans=isPresent(root->children[i],x);
        if(smallans!=ans && smallans==true)
            ans=smallans;
    }
    
   return ans;  // Write your code here
}
